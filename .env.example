# Nexzo MyProperty environment example
# Copy to .env.local or export via your secret manager

# Core services
NODE_ENV=development
PORTAL_BASE_URL=http://localhost:3000
ADMIN_BASE_URL=http://localhost:3100
API_BASE_URL=http://localhost:4000
AI_ORCHESTRATOR_URL=http://localhost:5000

# Database & caching
DATABASE_URL=postgres://postgres:postgres@localhost:5432/myproperty_dev
REDIS_URL=redis://localhost:6379/0
WAREHOUSE_DSN=bigquery://project.dataset

# Auth & security
JWT_SECRET=change-me
JWT_ISSUER=https://auth.nexzo.com
SESSION_SECRET=change-me-too
OAUTH_CLIENT_ID=your-client-id
OAUTH_CLIENT_SECRET=your-client-secret

# Integrations
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx
ORIGINFD_API_URL=https://api.originfd.dev
ORIGINFD_API_KEY=originfd-test-key
POSTMARK_API_KEY=postmark-key
TWILIO_ACCOUNT_SID=ACxxxx
TWILIO_AUTH_TOKEN=xxxx

# AI Providers
OPENAI_API_KEY=sk-openai
ANTHROPIC_API_KEY=anthropic-key
MISTRAL_API_KEY=mistral-key
EMBEDDING_MODEL=text-embedding-3-large

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
SENTRY_DSN=
LOG_LEVEL=info

# Feature flags
LAUNCHDARKLY_SDK_KEY=ld-key
